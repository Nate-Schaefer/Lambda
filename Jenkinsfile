pipeline {
    agent any

    stages {
        stage('Nick') {
            steps {
                echo 'Hello Nick'
            }
        }
        stage('Hageman') {
            steps {
                echo 'Hageman'
            }
        }
        stage('give') {
            steps {
                echo 'give me an'
            }
        }
        stage('job') {
            steps {
                echo 'apple job please'
            }
        }
    }
}
